                <script>
                    function selectPlan(plan) {
                        document.querySelectorAll('.plan-btn').forEach(btn => {
                            btn.classList.remove('active');
                        });
                        document.querySelector(`[data-plan="${plan}"]`).classList.add('active');
                        document.getElementById('buy-1day').style.display = plan === '1day' ? 'flex' : 'none';
                        document.getElementById('buy-7day').style.display = plan === '7day' ? 'flex' : 'none';
                        document.getElementById('buy-30day').style.display = plan === '30day' ? 'flex' : 'none';
                    }
                </script>

